    "@types/morgan": "^1.7.35",
    "@types/node": "^8.9.4",
    "@types/socket.io": "^1.4.31",
    "@types/winston": "^2.3.8",
    "cross-env": "^5.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "husky": "^0.14.3",
    "standard": "^11.0.0",
    "standard-version": "^4.3.0",
    "ts-node": "3.3.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
