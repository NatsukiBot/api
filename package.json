{
  "name": "@nightwatch/api",
  "version": "0.14.1",
  "description": "API for Nightwatch Discord bot.",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "0.0.40",
    "@types/express-rate-limit": "^2.9.3",
    "@types/helmet": "0.0.38",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.2",
    "@types/sinon": "^5.0.1",
    "@types/socket.io": "^1.4.31",
    "@types/supertest": "^2.0.5",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "1.13.7",
    "sinon": "^6.1.3",
    "source-map-support": "^0.5.6",
    "standard-version": "^4.3.0",
    "supertest": "^3.1.0",
    "ts-node": "7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@nightwatch/db": "^0.12.2",
    "accesscontrol": "^2.2.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.1",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "express-rate-limit": "^2.11.0",
    "express-status-monitor": "^1.0.1",
    "helmet": "^3.11.0",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^6.1.0",
    "jsonwebtoken": "^8.2.0",
    "mongo-morgan": "^2.0.3",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.3",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.10",
    "socket.io": "^2.0.4",
    "typeorm": "^0.2.5"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsc && nodemon --inspect --exec ts-node src/index.ts",
    "prod": "tsc && cross-env NODE_ENV=production nodemon --exec ts-node src/index.ts",
    "clean": "npm cache clear && rimraf -- dist",
    "docs": "cross-env typedoc --out public/docs src --name \"Nightwatch API\" --excludeExternals --mode file --theme public/theme",
    "release": "standard-version",
    "lint": "tslint --project . --format stylish",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nightwatch/api.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
