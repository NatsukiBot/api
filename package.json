    "typeorm": "^0.1.14",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsc && nodemon --inspect --exec ts-node src/index.ts",
    "prod": "tsc && cross-env NODE_ENV=production nodemon --exec ts-node src/index.ts",
    "clean": "npm cache clear && rimraf -- dist",
    "docs": "cross-env typedoc --out public/docs src --name \"Natsuki API\" --excludeExternals --mode file --theme public/theme",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NatsukiBot/api.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
